{
    "extends": "eslint",
    "env": {
        "es2020": true
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "2020"
    },
    "env": {
        "es6": true,
        "node": true
    },
    "overrides": [
        {
            "files": ["*.cjs"],
            "parserOptions": {
                "sourceType": "script"
            }
        }
    ]
}
